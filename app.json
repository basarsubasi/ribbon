{
  "expo": {
    "name": "ribbon",
    "slug": "ribbon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
     "supportsTablet" : false,
      "bundleIdentifier": "com.basarsubasi.ribbon",
      "buildNumber": "1.0.0"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.png",
        "backgroundColor": "#ffffff"
      },

      "blockedPermissions": [
      "android.permission.ACCESS_NETWORK_STATE",
      "android.permission.RECORD_AUDIO",
      "android.permission.ACCESS_WIFI_STATE" ],

      "package": "tr.com.basarsubasi.ribbon"
    },
    "web": {
      "favicon": "./assets/appicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "owner": "basarsubasi",
    "extra": {
      "eas": {
        "projectId": "a1854f89-3534-4340-a731-7cdc6451cff5"
      }
    }
  }
}
